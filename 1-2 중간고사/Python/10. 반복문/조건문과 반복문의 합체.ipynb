   "source": [
    "marks = [90, 25, 67, 45, 80]\n",
    "i = 0\n",
    "o = x = 0\n",
    "\n",
    "for mark in marks:\n",
    "\ti += 1\n",
    "\tprint(mark)\n",
    "\tif mark >= 60:\n",
    "\t\tprint(f\"{i}번 학생은 {mark}점으로 합격입니다.\")\n",
    "\t\to += 1\n",
    "\telse:\n",
    "\t\tprint(f\"{i}번 학생은 {mark}점으로 불합격입니다\")\n",
    "\t\tx += 1\n",
    "print(f\"합격: {o}명, 불합격: {x}명\")"
   ]
